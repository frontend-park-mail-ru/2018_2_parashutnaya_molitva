{
    "extends": "standard",
    "rules": {
        "semi": ["error", "always"],
        "indent": ["error", 4],
        "standard/no-callback-literal": "off"
    },
    "globals": {
        "fetch": false
    },
    "env": {
        "browser": true,
        "node": true,
        "jest": true
    }
}
